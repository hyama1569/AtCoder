   "source": [
    "n, a, b = map(int, input().split())\n",
    "res = (n // (a + b)) * a\n",
    "rest = n % (a + b)\n",
    "if rest <= a:\n",
    "    res += rest\n",
    "else:\n",
    "    res += a\n",
    "print(res)"
