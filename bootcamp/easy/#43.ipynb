   "source": [
    "w = input()\n",
    "d = {}\n",
    "\n",
    "for c in w:\n",
    "    if c not in d:\n",
    "        d[c] = 1\n",
    "    else:\n",
    "        d[c] += 1\n",
    "\n",
    "flag = False\n",
    "for v in d.values():\n",
    "    if v % 2 != 0:\n",
    "        flag = True\n",
    "\n",
    "if flag:\n",
    "    print(\"No\")\n",
    "else:\n",
    "    print(\"Yes\") "
   ]
  }
