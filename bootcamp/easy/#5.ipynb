    "n, m, c = map(int, input().split())\n",
    "b_ls = list(map(int, input().split()))\n",
    "a = [list(map(int, input().split())) for _ in range(n)]\n",
    "\n",
    "res = 0\n",
    "for a_ls in a:\n",
    "    s = 0\n",
    "    for a_i, b_i in zip(a_ls, b_ls):\n",
    "        s += a_i * b_i\n",
    "    s += c\n",
    "    if s > 0:\n",
    "        res += 1\n",
    "print(res)"
   ]
  }
