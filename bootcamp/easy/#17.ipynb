   "source": [
    "n = int(input())\n",
    "v = list(map(int, input().split()))\n",
    "v = sorted(v, reverse=False)\n",
    "res = (v[0] + v[1]) / 2\n",
    "if n > 2:\n",
    "    for i in range(2, n):\n",
    "        res = (res + v[i]) / 2\n",
    "    print(res)\n",
    "else:\n",
    "    print(res)"
