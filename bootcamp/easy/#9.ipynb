   "source": [
    "n = int(input())\n",
    "k = int(input())\n",
    "x = list(map(int, input().split()))\n",
    "med = int(k / 2)\n",
    "res = 0\n",
    "for i in x:\n",
    "    if i <= med:\n",
    "        res += 2 * i\n",
    "    else:\n",
    "        res += 2 * (k - i)\n",
    "print(res)"
