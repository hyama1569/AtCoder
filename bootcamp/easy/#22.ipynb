   "source": [
    "n, x = map(int, input().split())\n",
    "a = list(map(int, input().split()))\n",
    "a = sorted(a, reverse=False)\n",
    "\n",
    "if sum(a) < x:\n",
    "    print(len(a) - 1)\n",
    "else:\n",
    "    res = 0\n",
    "    for i in range(len(a)):\n",
    "        if x <= 0 or x < a[i]:\n",
    "            break\n",
    "        x -= a[i]\n",
    "        res += 1\n",
    "    print(res)"
