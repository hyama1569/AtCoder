   "source": [
    "n = int(input())\n",
    "x = list(map(int, input().split()))\n",
    "mean = sum(x) / len(x)\n",
    "mean_q = sum(x) // len(x)\n",
    "p = mean_q\n",
    "if mean - mean_q > 0.5:\n",
    "    p += 1\n",
    "res = 0\n",
    "for i in x:\n",
    "    res += (p - i)**2\n",
    "print(res)"
   ]
