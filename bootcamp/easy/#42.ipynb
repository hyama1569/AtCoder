   "source": [
    "n, m = map(int, input().split())\n",
    "foods = [0 for i in range(m)]\n",
    "\n",
    "for i in range(n):\n",
    "    inp = list(map(int, input().split()))\n",
    "    for num in inp[1:]:\n",
    "        foods[num-1] += 1\n",
    "\n",
    "res = 0\n",
    "for i in range(len(foods)):\n",
    "    if foods[i] == n:\n",
    "        res += 1\n",
    "print(res)"
   ]
  }
