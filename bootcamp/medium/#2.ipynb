   "source": [
    "s = input()\n",
    "\n",
    "cumB = []\n",
    "tmp = 0\n",
    "for c in s:\n",
    "    if c == \"B\":\n",
    "        tmp += 1\n",
    "    cumB.append(tmp)\n",
    "\n",
    "res = 0\n",
    "for i in range(len(s)):\n",
    "    if s[i] == 'W':\n",
    "        res += cumB[i]\n",
    "print(res)"
   ]
